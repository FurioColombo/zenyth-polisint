{"ast":null,"code":"import \"core-js/modules/web.timers.js\";\nimport * as Tone from \"tone\";\nimport OscHost from \"./Synth/Oscillators/OscHost.vue\";\nimport FilterHost from \"./Synth/Filters/FilterHost.vue\";\nimport FxHost from \"./Synth/Effects/FxHost.vue\";\nimport ToolHost from \"./Synth/Tools/ToolsHost.vue\";\nimport ModHost from \"./Synth/Modulators/ModHost.vue\"; // Envelope Strip\nimport PianoKeyboard from \"./Synth/Keyboard/PianoKeyboard.vue\";\nexport default {\n  name: \"mainHost\",\n  components: {\n    OscHost: OscHost,\n    FilterHost: FilterHost,\n    FxHost: FxHost,\n    ToolHost: ToolHost,\n    ModHost: ModHost,\n    PianoKeyboard: PianoKeyboard\n  },\n  data: function data() {\n    return {\n      draggedInfo: null\n    };\n  },\n  mounted: function mounted() {\n    Tone.Transport.start(\"+0.1\");\n  },\n  methods: {\n    // -------------- INIT ROUTING (connections) ------------\n    initOscConnections: function initOscConnections() {\n      this.$refs.oscHost.initConnections();\n    },\n    handleNewOsc: function handleNewOsc() {\n      //initialize volEnvelope on all synth when a new oscillator is added\n      this.$refs.modHost.handleNewOsc();\n    },\n    handleMouseUp: function handleMouseUp(e) {\n      var _this = this;\n      //Actions to execute when mouseup happens on any part of the screen\n      this.$refs.pianoKeyboard.handleMouseUp();\n      this.$refs.modHost.handleMouseUp(e);\n      this.$refs.fxHost.handleMouseUp(e);\n\n      //If something has been dragged and not claimed after 100ms infos are deleted\n      if (this.draggedInfo) {\n        setTimeout(function () {\n          return _this.deleteDraggedInfo(\"MainHost\");\n        }, 100);\n      }\n    },\n    handleMouseMove: function handleMouseMove(e) {\n      //console.log(this.$refs.modHost)\n      this.$refs.modHost.handleMouseMove(e);\n      this.$refs.fxHost.handleMouseMove(e);\n    },\n    noteDownHandle: function noteDownHandle(note) {\n      this.$refs[\"toolsHost\"].setOscilloscopeTiming(note);\n    },\n    noteUpHandle: function noteUpHandle(note) {}\n  }\n};","map":{"version":3,"mappings":";AAqBA,OAAO,KAAKA,IAAG,MAAO,MAAM;AAC5B,OAAOC,OAAM,MAAO,iCAAiC;AACrD,OAAOC,UAAS,MAAO,gCAAgC;AACvD,OAAOC,MAAK,MAAO,4BAA4B;AAC/C,OAAOC,QAAO,MAAO,6BAA6B;AAClD,OAAOC,OAAM,MAAO,gCAAgC,EAAE;AACtD,OAAOC,aAAY,MAAO,oCAAoC;AAE9D,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVP,OAAO,EAAPA,OAAO;IACPC,UAAU,EAAVA,UAAU;IACVC,MAAM,EAANA,MAAM;IACNC,QAAQ,EAARA,QAAQ;IACRC,OAAO,EAAPA,OAAO;IACPC,aAAa,EAAbA;EAEF,CAAC;EACDG,IAAI,kBAAG;IACL,OAAO;MACLC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,qBAAG;IACRX,IAAI,CAACY,SAAS,CAACC,KAAK,CAAC,MAAM;EAC7B,CAAC;EAEDC,OAAO,EAAE;IACP;IACAC,kBAAkB,gCAAE;MAClB,IAAI,CAACC,KAAK,CAACC,OAAO,CAACC,eAAe,EAAC;IACrC,CAAC;IACDC,YAAY,0BAAE;MACZ;MACA,IAAI,CAACH,KAAK,CAACI,OAAO,CAACD,YAAY,EAAE;IACnC,CAAC;IACDE,aAAa,yBAACC,CAAC,EAAE;MAAA;MACf;MACA,IAAI,CAACN,KAAK,CAACO,aAAa,CAACF,aAAa,EAAE;MACxC,IAAI,CAACL,KAAK,CAACI,OAAO,CAACC,aAAa,CAACC,CAAC,CAAC;MACnC,IAAI,CAACN,KAAK,CAACQ,MAAM,CAACH,aAAa,CAACC,CAAC,CAAC;;MAElC;MACA,IAAI,IAAI,CAACZ,WAAW,EAAE;QACpBe,UAAU,CAAC;UAAA,OAAM,KAAI,CAACC,iBAAiB,CAAC,UAAU,CAAC;QAAA,GAAE,GAAG,CAAC;MAC3D;IACF,CAAC;IACDC,eAAe,2BAACL,CAAC,EAAC;MAChB;MACA,IAAI,CAACN,KAAK,CAACI,OAAO,CAACO,eAAe,CAACL,CAAC,CAAC;MACrC,IAAI,CAACN,KAAK,CAACQ,MAAM,CAACG,eAAe,CAACL,CAAC,CAAC;IAEtC,CAAC;IACDM,cAAc,0BAACC,IAAI,EAAE;MACnB,IAAI,CAACb,KAAK,CAAC,WAAW,CAAC,CAACc,qBAAqB,CAACD,IAAI,CAAC;IACrD,CAAC;IAGDE,YAAY,wBAACF,IAAI,EAAE,CACnB;EAEF;AACF,CAAC","names":["Tone","OscHost","FilterHost","FxHost","ToolHost","ModHost","PianoKeyboard","name","components","data","draggedInfo","mounted","Transport","start","methods","initOscConnections","$refs","oscHost","initConnections","handleNewOsc","modHost","handleMouseUp","e","pianoKeyboard","fxHost","setTimeout","deleteDraggedInfo","handleMouseMove","noteDownHandle","note","setOscilloscopeTiming","noteUpHandle"],"sourceRoot":"","sources":["/Users/eliapirrello/Documents/GitHub/zenyth/src/views/MainHost.vue"],"sourcesContent":["<template>\n  <div class=\"appContainer\" @mouseup=\"handleMouseUp\" @mousemove=\"handleMouseMove\">\n    <div class=\"grid\">\n    <img className=\"app_logo\" src=\"@/img/zenyth_logo.png\" draggable=\"false\">\n    <div class=\"mainHost\">\n      <OscHost ref=\"oscHost\" @newOsc=\"handleNewOsc\" />\n      <FilterHost ref=\"filterHost\" @filterInitialized=\"initOscConnections\"/>\n      <FxHost ref=\"fxHost\"/>\n      <ToolHost ref=\"toolsHost\" />\n      <ModHost ref=\"modHost\"/>\n      <PianoKeyboard\n        ref=\"pianoKeyboard\"\n        @noteDown=\"noteDownHandle\"\n        @noteUp=\"noteUpHandle\"\n      />\n    </div>\n  </div>\n</div>\n</template>\n\n<script>\nimport * as Tone from \"tone\";\nimport OscHost from \"./Synth/Oscillators/OscHost.vue\";\nimport FilterHost from \"./Synth/Filters/FilterHost.vue\";\nimport FxHost from \"./Synth/Effects/FxHost.vue\";\nimport ToolHost from \"./Synth/Tools/ToolsHost.vue\";\nimport ModHost from \"./Synth/Modulators/ModHost.vue\"; // Envelope Strip\nimport PianoKeyboard from \"./Synth/Keyboard/PianoKeyboard.vue\";\n\nexport default {\n  name: \"mainHost\",\n  components: {\n    OscHost,\n    FilterHost,\n    FxHost,\n    ToolHost,\n    ModHost,\n    PianoKeyboard,\n\n  },\n  data() {\n    return {\n      draggedInfo: null,\n    };\n  },\n  mounted() {\n    Tone.Transport.start(\"+0.1\")\n  },\n\n  methods: {\n    // -------------- INIT ROUTING (connections) ------------\n    initOscConnections(){\n      this.$refs.oscHost.initConnections()\n    },\n    handleNewOsc(){\n      //initialize volEnvelope on all synth when a new oscillator is added\n      this.$refs.modHost.handleNewOsc();\n    },\n    handleMouseUp(e) {\n      //Actions to execute when mouseup happens on any part of the screen\n      this.$refs.pianoKeyboard.handleMouseUp();\n      this.$refs.modHost.handleMouseUp(e);\n      this.$refs.fxHost.handleMouseUp(e);\n\n      //If something has been dragged and not claimed after 100ms infos are deleted\n      if (this.draggedInfo) {\n        setTimeout(() => this.deleteDraggedInfo(\"MainHost\"), 100);\n      }\n    },\n    handleMouseMove(e){\n      //console.log(this.$refs.modHost)\n      this.$refs.modHost.handleMouseMove(e);\n      this.$refs.fxHost.handleMouseMove(e);\n\n    },\n    noteDownHandle(note) {\n      this.$refs[\"toolsHost\"].setOscilloscopeTiming(note);\n    },\n\n\n    noteUpHandle(note) {\n    },\n\n  },\n};\n</script>\n\n<style>\n.mainHost {\n  display: grid; /* no more flex display because we need Env Strip to be longer */\n  grid-template-rows: 55% 30% 15%; /* grid display row sizes*/\n  grid-template-columns: 20% 55% 25%; /*  ...   ...   column sizes fr is like unit */\n  width: 100%;\n  justify-content: center;\n  aspect-ratio: 4 / 3;\n  background-color: transparent;\n}\n.appContainer {\n  text-align: center;\n  width: 80vw;\n  height: 100vh;\n  margin: auto;\n}\n.grid{\n  display: grid;\n    grid-template-columns: 15% 85%;\n    justify-content: center;\n    justify-items: center;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}